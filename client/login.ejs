              <form method="post" action="/api/login" data-bs-theme="light" onsubmit="return onSubmitForm(event);" id="login-form">
                <div class="mb-3">
                  <input class="shadow form-control" type="email" name="email" placeholder="Email" required="">
                </div>
                <div class="mb-3">
                  <input class="shadow form-control" type="password" name="password" placeholder="Password" required="">
                </div>
                <div class="mb-3">
                    <div class="g-recaptcha" data-sitekey="6LforXInAAAAADvSaLmlYGLAA0fXqgyJs7ni0nnU"></div>
                    <input type="hidden" id="recaptcha-response" name="g-recaptcha-response">

                <!-- Add the following script to handle form submission -->
                <script>
                  async function onSubmitForm(event) {
                    event.preventDefault();
                    const recaptchaResponse = await grecaptcha.getResponse();

                    // Check if the response is not empty
                    if (!recaptchaResponse) {
                      alert("Please complete the reCAPTCHA verification.");
                      return false;
                    }

                    // Set the reCAPTCHA response in a hidden input field
                    const recaptchaInput = document.createElement('input');
                    recaptchaInput.setAttribute('type', 'hidden');
                    recaptchaInput.setAttribute('name', 'g-recaptcha-response');
                    recaptchaInput.setAttribute('value', recaptchaResponse);
                    document.getElementById('login-form').appendChild(recaptchaInput);

                    // Proceed with form submission
                    document.getElementById('login-form').submit();
                    return true;
                  }
                </script>             
                </div>
                <div class="mb-3">
                  <% if (error) { %>
